macro include {-> <stdio.h> <-}
macro include {-> <stdlib.h> <-}

def print(str: [&u8])
  spawn {-> 
    printf("printed: %s\n", str);
    free(str); // Memory release required as values ​​are reallocated
  <-}
end

our msg: [&u8] = spawn our {->"My name is Lucas!";<-}
pendence print
arg [!str] msg
call

spawn our {->
  free(msg);
  return 0;
<-}